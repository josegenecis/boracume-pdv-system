  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build:prod && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "electron": "electron electron/main.js",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron electron/main.js\"",
    "build:electron": "npm run build && electron-builder",
    "dist:electron": "npm run build && electron-builder --publish=never",
    "pack:electron": "electron-builder --dir",
    "build:desktop-portable": "node build-desktop-portable.cjs"
